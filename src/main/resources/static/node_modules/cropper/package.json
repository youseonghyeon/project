    "babel-plugin-istanbul": "^5.2.0",
    "chai": "^4.2.0",
    "change-case": "^3.1.0",
    "cpy-cli": "^2.0.0",
    "create-banner": "^1.0.0",
    "cssnano": "^4.1.10",
    "del-cli": "^3.0.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "husky": "^3.0.8",
    "jquery": "^3.3.1",
    "karma": "^4.3.0",
    "karma-chai": "^0.1.0",
